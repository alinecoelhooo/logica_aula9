programa
{
	  inteiro valor
          inteiro cont
          funcao cadeia geraTabuada(inteiro pValor)     
	{
		para ( cont = 1;  cont<=10; cont++ )
       		  escreva (pValor, " x ", cont," = ", (pValor * cont), "\n")
		retorne ""		
	}	 
	 funcao inicio()
	{
		escreva("Informe um valor inteiro: ")	
		leia(valor)
		geraTabuada(valor)
	}
}
